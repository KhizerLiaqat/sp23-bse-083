<h2>Order List</h2>
<table>
  <thead>
    <tr>
      <th>Order ID</th>
      <th>Customer Name</th>
      <th>Address</th>
      <th>Total Amount</th>
      <th>Order Date</th>
    </tr>
  </thead>
  <tbody>
    <% orders.forEach(order => { %>
      <tr>
        <td><%= order.orderId %></td>
        <td><%= order.customerInfo.name %></td>
        <td><%= order.customerInfo.street %>, <%= order.customerInfo.city %>, <%= order.customerInfo.postalCode %></td>
        <td><%= order.totalAmount %></td>
        <td><%= order.orderDate.toLocaleString() %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
